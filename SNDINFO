//===========================================================================
//
// PROPHESSOR
//
//===========================================================================

$playersound	prophessor	male	*death		pspldeth
$playersound	prophessor	male	*xdeath		pspdiehi
$playersound	prophessor	male	*gibbed		psslop
$playersound	prophessor	male	*pain100	psplpain
$playersounddup	prophessor	male	*pain75		*pain100
$playersounddup	prophessor	male	*pain50		*pain100
$playersounddup	prophessor	male	*pain25		*pain100
$playersound	prophessor	male	*grunt		psoof
$playersounddup	prophessor	male	*land		*grunt
$playersound	prophessor	male	*jump		psjump
$playersound	prophessor	male	*fist		pspunch
$playersound	prophessor	male	*usefail	psnoway

