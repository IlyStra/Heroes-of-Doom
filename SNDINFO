$playersound	irondroog	male	*death		sprites/classes/irondroog/sounds/ISPLDETH
$playersound	irondroog	male	*xdeath		sprites/classes/irondroog/sounds/ISPLDETH
$playersound	irondroog	male	*gibbed		sprites/classes/irondroog/sounds/ISPLDETH
$playersound	irondroog	male	*pain100	sprites/classes/irondroog/sounds/ISPLPAIN
$playersounddup	irondroog	male	*pain75		*pain100
$playersounddup	irondroog	male	*pain50		*pain100
$playersounddup	irondroog	male	*pain25		*pain100
$playersound	irondroog	male	*grunt		sprites/classes/irondroog/sounds/isoof
$playersounddup	irondroog	male	*land		*grunt
$playersound	irondroog	male	*jump		sprites/classes/irondroog/sounds/isjump
$playersound	irondroog	male	*fist		sprites/classes/irondroog/sounds/ispunch
$playersound	irondroog	male	*usefail	sprites/classes/irondroog/sounds/isnoway
$playersound	irondroog	male	w_pkup		sprites/classes/irondroog/sounds/iswpnup		// Pickup weapon
$playersound	irondroog	male	p_pkup		sprites/classes/irondroog/sounds/isgetpow	// Pickup powerup
$playersound	irondroog	male	i_pkup		sprites/classes/irondroog/sounds/isitemup	// Pickup item
$playersound	irondroog	male	k_pkup		sprites/classes/irondroog/sounds/isitemup	// Pickup key

